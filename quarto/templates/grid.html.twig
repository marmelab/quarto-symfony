<div class="grid">
  {% for row in grid %}
    <div class="grid-row">
      {% for value in row %}
          <div class="default-box grid-box {{ value != '.' ? 'filled' : '' }} z-depth-1">
              <span class="grid-box-value">{{value}}</span>          
          </div>
      {% endfor %}
    </div>
  {% endfor %}
</div>
